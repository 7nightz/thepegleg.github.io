  <script>
    if (localStorage.pinned == undefined) {
      localStorage.pinned = "{}"
    }
    var pinned = JSON.parse(localStorage.pinned);

    let elements = document.querySelectorAll(".searchable");
    for (let i = 0; i < elements.length; i++) {
      if (pinned[elements[i].dataset.name] == undefined) {
        pinned[elements[i].dataset.name] = false;
      }
    }
    updatePin()
    sort()

    function search() {
      let elements = document.querySelectorAll(".searchable");
      let search = document.querySelector(".game-search").value;
      for (let i = 0; i < elements.length; i++) {
        if (elements[i].dataset.name.includes(search)) {
          elements[i].style.display = "";
        } else {
          elements[i].style.display = "none";
        }
      }
    }

    function fav(str) {
      pinned = JSON.parse(localStorage.pinned);
      pinned[str] = !pinned[str];
      localStorage.pinned = JSON.stringify(pinned);
      sort();
      updatePin();
    }

    function updatePin() {
      let elements = document.querySelectorAll(".pin-button");
      for (let i = 0; i < elements.length; i++) {
        elements[i].parentNode.parentNode.dataset.name
        if (pinned[elements[i].parentNode.parentNode.dataset.name]) {
          elements[i].innerText = "[pinned!]"
        } else {
          elements[i].innerText = "[pin?]"
        }
      }
    }


    function sort() {
      let items = document.querySelectorAll('.searchable');
      let itemsArr = [];
      for (i = 0; i < items.length; ++i) {
        itemsArr.push(items[i]);
      }

      pinned = JSON.parse(localStorage.pinned);

      itemsArr.sort(function(a, b) {
        if (a.dataset == undefined || b.dataset == undefined || a.dataset.view == b.dataset.view) {
          return 0;
        }
        if (pinned[a.dataset.name] && !pinned[b.dataset.name]) {
          return -1;
        }
        if (!pinned[a.dataset.name] && pinned[b.dataset.name]) {
          return 1;
        }
        let bigger = JSON.parse(a.dataset.view) < JSON.parse(b.dataset.view);
        return bigger ? 1 : -1;
      });

      for (i = 0; i < itemsArr.length; ++i) {
        let parent = items[0].parentNode;
        let aft = document.getElementById("sort-anchor");
        let oldChild = parent.removeChild(itemsArr[i]);
        parent.insertBefore(oldChild, aft);
      }
    }
  </script>
